@model IEnumerable<CheeseMVC.Models.Menu>

<h1>Menus</h1>

<p><a asp-controller="Menu" asp-action="Add">Add Menu</a></p>

<ul>
    @foreach (var menu in Model)
    {
        <li>
            <a asp-controller="Menu"
               asp-action="ViewMenu"
               asp-route-id="@menu.ID"
               >@menu.Name</a>
        </li>

    }
</ul>